      "cell_type": "code",
      "source": [
        "# Función para intentar convertir a numérico las columnas que puedan serlo\n",
        "def convertir_a_numerico(col):\n",
        "    try:\n",
        "        # Intentar convertir la columna a numérico\n",
        "        return pd.to_numeric(col, errors='coerce')  # 'coerce' convierte a NaN cuando no puede\n",
        "    except ValueError:\n",
        "        # Si no puede, la devuelve tal cual está\n",
        "        return col\n",
        "\n",
        "# Aplicar la función a cada columna del DataFrame\n",
        "df_unido = df_unido.apply(convertir_a_numerico)"
      ],
      "metadata": {
